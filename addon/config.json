{
  "name": "AB Transaction Importer",
  "version": "1.0.0",
  "slug": "ab-trx-importer",
  "description": "Import bank transactions to Actual Budget",
  "url": "https://github.com/yourusername/ab-trx-importer-frontend",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ports": {
    "8000/tcp": 8000,
    "5173/tcp": 5173
  },
  "ports_description": {
    "8000/tcp": "Backend API",
    "5173/tcp": "Frontend web interface"
  },
  "map": ["config:rw"],
  "options": {
    "frontend_port": 5173,
    "backend_port": 8000
  },
  "schema": {
    "frontend_port": "int?",
    "backend_port": "int?"
  },
  "build_from": {
    "aarch64": "ghcr.io/hassio-addons/base/aarch64:latest",
    "armhf": "ghcr.io/hassio-addons/base/armhf:latest",
    "armv7": "ghcr.io/hassio-addons/base/armv7:latest",
    "amd64": "ghcr.io/hassio-addons/base/amd64:latest",
    "i386": "ghcr.io/hassio-addons/base/i386:latest"
  }
}
